<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/relativelayoutcircle"
        android:layout_gravity="center"
        android:background="@drawable/circleimageview"
        android:layout_width="320dp"
        android:layout_height="320dp">


    </RelativeLayout>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ic_ambulance_24dp"
        app:layout_anchor="@+id/relativelayoutcircle"
        app:layout_anchorGravity="start|center" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ic_child_care_blue_24dp"
        app:layout_anchor="@+id/relativelayoutcircle"
        app:layout_anchorGravity="top|center" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ic_notifications_active_blue_24dp"
        app:layout_anchor="@+id/relativelayoutcircle"
        app:layout_anchorGravity="bottom|center" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ic_patient_history_24dp"
        app:layout_anchor="@+id/relativelayoutcircle"
        app:layout_anchorGravity="end|center" />


</androidx.coordinatorlayout.widget.CoordinatorLayout>